<p id="notice"><%= notice %></p>

<h1>Clubhouses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clubhouses.each do |clubhouse| %>
      <tr>
        <td><%= link_to 'Show', clubhouse %></td>
        <td><%= link_to 'Edit', edit_clubhouse_path(clubhouse) %></td>
        <td><%= link_to 'Destroy', clubhouse, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Clubhouse', new_clubhouse_path %>
